recordtype(stringin) {
    %#include "epicsTypes.h"
    %#include "link.h"
    %#include "epicsMutex.h"
    %#include "ellLib.h"
    %#include "epicsTime.h"
    field(NAME, DBF_STRING) {
        special(SPC_NOMOD)
        prompt("Record Name")
        size(61)
    }
    field(DESC, DBF_STRING) {
        size(41)
        prompt("Descriptor")
        promptgroup("10 - Common")
    }
    field(ASG, DBF_STRING) {
        size(29)
        prompt("Access Security Group")
        promptgroup("10 - Common")
        special(SPC_AS)
    }
    field(SCAN, DBF_MENU) {
        prompt("Scan Mechanism")
        menu(menuScan)
        interest(1)
        special(SPC_SCAN)
        promptgroup("20 - Scan")
    }
    field(PINI, DBF_MENU) {
        interest(1)
        menu(menuPini)
        prompt("Process at iocInit")
        promptgroup("20 - Scan")
    }
    field(PHAS, DBF_SHORT) {
        special(SPC_SCAN)
        promptgroup("20 - Scan")
        prompt("Scan Phase")
        interest(1)
    }
    field(EVNT, DBF_STRING) {
        prompt("Event Name")
        size(40)
        interest(1)
        special(SPC_SCAN)
        promptgroup("20 - Scan")
    }
