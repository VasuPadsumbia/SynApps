        interest(2)
        prompt("Alarm Ack Transient")
        special(SPC_NOMOD)
        initial("YES")
        promptgroup("70 - Alarm")
        menu(menuYesNo)
    }
    field(DISS, DBF_MENU) {
        promptgroup("70 - Alarm")
        menu(menuAlarmSevr)
        prompt("Disable Alarm Sevrty")
        interest(1)
    }
